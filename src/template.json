{
  "$schema": "vscode://schemas/color-theme",
  "name": "{theme_name}",
  "type": "{theme_type}",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "{fg_dim}"
      }
    },
    {
      "name": "No highlighting",
      "scope": [
        "constant",
        "constant.character.escape",
        "entity.other.attribute-name.id",
        "support.constant",
        "string"
      ],
      "settings": {
        "foreground": "{fg}"
      }
    },
    {
      "name": "Errors",
      "scope": [
        "entity.name.exception",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "{red_1}"
      }
    },
    {
      "name": "Sections and headings",
      "scope": [
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "{fg}",
        "fontStyle": "bold"
      }
    }
  ],
  "colors": {
    "descriptionForeground": "{fg}",
    "disabledForeground": "{fg_dim}",
    "errorForeground": "{red_1}",
    "focusBorder": "{border}",
    "foreground": "{fg}",
    "icon.foreground": "{fg}",
    "selection.background": "{selection_bg}",
    "widget.border": "{border}",

    "activityBar.activeBorder": "{ui_bg}",
    "activityBar.activeFocusBorder": "{border}",
    "activityBar.background": "{ui_bg}",
    "activityBar.border": "{border}",
    "activityBar.foreground": "{fg}",
    "activityBar.inactiveForeground": "{fg_faint}",

    "activityBarBadge.background": "{badge_bg}",
    "activityBarBadge.foreground": "{badge_fg}",

    "badge.background": "{badge_bg}",
    "badge.foreground": "{badge_fg}",

    "breadcrumb.background": "{ui_bg}",
    "breadcrumb.foreground": "{fg}",
    "breadcrumb.focusForeground": "{fg}",
    "breadcrumbPicker.background": "{hover_bg}",

    "button.background": "{button_bg}d8",
    "button.foreground": "{button_fg}",
    "button.border": "{black}",
    "button.hoverBackground": "{button_bg}",
    "button.secondaryBackground": "{invisible}",
    "button.secondaryForeground": "{fg}",
    "button.secondaryHoverBackground": "{fg}10",

    "checkbox.border": "{border}",
    "dropdown.border": "{border}",

    "debugIcon.breakpointForeground": "{red_1}",
    "debugToolBar.background": "{hover_bg}",

    "editor.background": "{bg_1}",
